<hardware name="g.usbamp" version="B" serial="UB-2009.07.07">
    <mode> master </mode>
    <device_settings>
      <sampling_rate> 1200 </sampling_rate>
      <measurement_channels nr="16" names="eeg" type="eeg" />

      <filter type="chebyshev" order="8" f_low="0.5" f_high="100"/>
      <notch f_center="50"/>
      <blocksize> 8 </blocksize>
      <operation_mode> normal </operation_mode>

      <shortcut> off </shortcut>
      <trigger_line> off </trigger_line>

      <usbamp_master> yes </usbamp_master>

      <common_ground>
        <gnd block="a" value="1" />
        <gnd block="b" value="1" />
        <gnd block="c" value="1" />
        <gnd block="d" value="1" />
      </common_ground>

      <common_reference>
        <cr block="a" value="1" />
        <cr block="b" value="1" />
        <cr block="c" value="1" />
        <cr block="d" value="1" />
      </common_reference>
    </device_settings>

  </hardware>

  <hardware name="g.usbamp" version="B" serial="UB-2009.10.11">
    <mode> slave </mode>
    <device_settings>
      <sampling_rate> 1200 </sampling_rate>
      <measurement_channels nr="16" names="eeg" type="eeg" />

      <filter type="chebyshev" order="8" f_low="0.5" f_high="100"/>
      <notch f_center="50"/>
      <blocksize> 8 </blocksize>
      <operation_mode> normal </operation_mode>

      <shortcut> off </shortcut>
      <trigger_line> off </trigger_line>

      <usbamp_master> no </usbamp_master>

      <common_ground>
        <gnd block="a" value="1" />
        <gnd block="b" value="1" />
        <gnd block="c" value="1" />
        <gnd block="d" value="1" />
      </common_ground>

      <common_reference>
        <cr block="a" value="1" />
        <cr block="b" value="1" />
        <cr block="c" value="1" />
        <cr block="d" value="1" />
      </common_reference>
    </device_settings>

  </hardware>

