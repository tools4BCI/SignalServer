    <mode> master </mode>
    <device_settings>
      <sampling_rate> 500 </sampling_rate>
      <measurement_channels nr="1" names="eeg" type="eeg" />
      <blocksize> 2 </blocksize>
    </device_settings>

    <channel_settings>
      <selection>
        <ch nr="01" name="A" type="eeg" />
      </selection>
    </channel_settings>
  </hardware>
  
